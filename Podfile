source 'https://github.com/CocoaPods/Specs.git'
platform :ios, '10.0'
use_frameworks!

target 'Podcast' do

	# almofire pods
    pod 'ACBAVPlayer'
    pod 'Alamofire', '~> 4.7'
    pod 'SDWebImage', '~> 4.0'
    pod 'FeedKit', '~> 8.0'
    # firbase pods
    pod 'Firebase'
    pod 'Firebase/Database'
    pod 'Firebase/Storage'
    pod 'Firebase/Auth'
    pod 'Firebase/Firestore'

end
